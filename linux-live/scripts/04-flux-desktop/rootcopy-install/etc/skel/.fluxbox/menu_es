[begin] (Menú de escritorio)
   [exec] (Terminal) { fbstartupnotify && xterm -ls }
   [exec] (Gestor de archivos) { fbstartupnotify && pcmanfm }
   [exec] (Navegador web) { fbstartupnotify && fbliveapp firefox-esr }
   [exec] (Editor de texto) { fbstartupnotify && mousepad }
   [exec] (Calculadora) { fbstartupnotify && galculator }
   [exec] (Gestor de red) { fbstartupnotify && connman-gtk }
   [exec] (Ejecutar) { fbappselect }
   [separator]
   [workspaces] (Espacios de trabajo     ...)
   [submenu] (Resolución de pantalla  ...) {}
      [include] (~/.fluxbox/menu_resolution)
   [end]
   [submenu] (Disposición del teclado ...) {}
      [exec] (English) { fbsetkb us } </usr/share/icons/locolor/16x16/flags/flag_usa.png>
      [exec] (Español) { fbsetkb es } </usr/share/icons/locolor/16x16/flags/flag_spain.png>
   [end]
   [exec] (Salir) { fblogout }
[end]
